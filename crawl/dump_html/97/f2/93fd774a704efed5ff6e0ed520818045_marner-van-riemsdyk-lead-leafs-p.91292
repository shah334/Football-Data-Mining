    </div>
</section>


<!-- Scoreboard Game Date Markup -->
<script type="text/ng-template" id="scoreboard-game-date.html">
    <!-- NFL Date Cells -->
    <div class="date" ng-if="currentSport == 'nfl'">
        <div class="season" ng-if="day.SeasonType.indexOf('pre-') > -1">PRESEASON</div>
        <strong ng-if="day.Week < 18">WEEK</strong>
        <div class="month-daynumber" ng-if="day.Week < 18">{{day.Week}}</div>

        <strong ng-if="day.Week == 18">WC</strong>
        <strong ng-if="day.Week == 19">DIV</strong>
        <strong ng-if="day.Week == 20">CONF</strong>
        <div class="month-daynumber" ng-if="day.Week > 17 && day.Week < 20">Round</div>
        <div class="month-daynumber" ng-if="day.Week == 20">CHAMP</div>

        <strong ng-if="day.Week == 21">PRO</strong>
        <strong ng-if="day.Week == 22">SUPER</strong>
        <div class="month-daynumber" ng-if="day.Week > 20">BOWL</div>
    </div>
    <!-- CFL Date Cells -->
    <div class="date" ng-if="currentSport == 'cfl'">
        <div class="season" ng-if="day.SeasonType.indexOf('pre-') > -1">PRESEASON</div>

        <strong ng-if="day.Week < 21">WEEK</strong>
        <div class="month-daynumber" ng-if="day.Week < 21">{{day.Week}}</div>

        <strong ng-if="day.Week == 21 || day.Week == 22">DIV</strong>
        <div class="month-daynumber" ng-if="day.Week == 21">SEMIS</div>
        <div class="month-daynumber" ng-if="day.Week == 22">FINALS</div>

        <strong ng-if="day.Week == 23">GREY</strong>
        <div class="month-daynumber" ng-if="day.Week == 23">CUP</div>
    </div>
    <!-- MLS Date Cells -->
    <div class="date" ng-if="currentSport == 'mls'">
        <!-- Regular Season Week cell -->
        <strong ng-if="day.Games[0].Type == 'RegularSeason' || day.Games[0].TypeDetails == 'All-Star'">WEEK</strong>
        <div class="month-daynumber" ng-if="day.Games[0].Type == 'RegularSeason' || day.Games[0].TypeDetails == 'All-Star'">{{day.Week}}</div>

        <!-- PostSeason Date cells -->
        <strong ng-if="day.Games[0].TypeDetails && day.Games[0].Type != 'RegularSeason' && day.Games[0].TypeDetails != 'All-Star'">{{day.Games[0].TypeDetails | split:' ':0}}</strong>
        <div class="month-daynumber" ng-if="day.Games[0].TypeDetails && day.Games[0].Type != 'RegularSeason' && day.Games[0].TypeDetails != 'All-Star'">{{day.Games[0].TypeDetails | split:' ':1}}</div>

    </div>
    <div class="date" ng-if="currentSport != 'nfl' || currentSport != 'cfl' || currentSport == 'euro'">
        <strong>{{day.Date | amDateFormat:'ddd'}}</strong>
        <div class="month-daynumber">{{day.Date | amDateFormat:'MMM DD'}}</div>
    </div>
</script>

<!-- Scoreboard Game Score Markup -->
<script type="text/ng-template" id="scoreboard-game-score.html">
    <a ng-href="/{{currentSportUrl}}/game/{{game.Id || game.SeoId}}">
