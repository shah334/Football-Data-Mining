                            <span
                                ng-switch-when="mlb"
                                ng-if="(game.Away.SeriesWin > game.Away.SeriesLost && game.TypeDetails != 'Division Playoff' && game.Away.SeriesWin < 4) || (game.Away.SeriesWin > game.Away.SeriesLost && game.TypeDetails == 'Division Playoff' && game.Away.SeriesWin < 3)">{{ game.Away.Team.Acronym }} Leads</span>
